{
  "name": "@prefabs.tech/saas-react",
  "version": "0.28.0",
  "description": "Saas plugin for react",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/PrefabsTechSaasReact.es.js",
      "require": "./dist/PrefabsTechSaasReact.umd.js"
    },
    "./dist/PrefabsTechSaasReact.css": "./dist/saas-react.css"
  },
  "main": "dist/PrefabsTechSaasReact.umd.js",
  "module": "dist/PrefabsTechSaasReact.es.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "snapshot:update": "vitest --environment jsdom run --update",
    "sort-package": "npx sort-package-json",
    "test": "pnpm build && vitest --environment jsdom run --coverage --pass-with-no-tests",
    "test:component": "vitest --environment jsdom run component/ --pass-with-no-tests",
    "test:snapshot": "vitest --environment jsdom run snapshot/ --pass-with-no-tests",
    "test:unit": "vitest --environment jsdom run unit/ --pass-with-no-tests",
    "typecheck": "tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "axios": "1.12.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@prefabs.tech/eslint-config": "0.5.0",
    "@prefabs.tech/react-config": "0.70.0",
    "@prefabs.tech/react-form": "0.70.0",
    "@prefabs.tech/react-i18n": "0.70.0",
    "@prefabs.tech/react-ui": "0.70.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "24.10.0",
    "@types/react": "18.3.4",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.39.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "56.0.1",
    "jsdom": "25.0.1",
    "prettier": "3.8.1",
    "primereact": "9.6.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.28.0",
    "react-toastify": "10.0.5",
    "typescript": "5.7.3",
    "vite": "6.3.5",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@prefabs.tech/react-config": ">=0.70.0",
    "@prefabs.tech/react-form": ">=0.70.0",
    "@prefabs.tech/react-i18n": ">=0.70.0",
    "@prefabs.tech/react-ui": ">=0.70.0",
    "primereact": ">=9.6.1",
    "react": ">=18.2",
    "react-dom": ">=18.3.1",
    "react-router-dom": ">=6.28.0",
    "react-toastify": ">=10.0"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
