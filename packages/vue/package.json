{
  "name": "@12deg/saas-vue",
  "version": "0.18.0",
  "description": "Saas plugin for vue",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/12degSaasVue.es.js",
      "require": "./dist/12degSaasVue.umd.js"
    },
    "./dist/12degSaasVue.css": "./dist/saas-vue.css"
  },
  "main": "dist/12degSaasVue.umd.js",
  "module": "dist/12degSaasVue.es.js",
  "types": "./dist/src/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue",
    "lint:fix": "eslint . --ext .vue --fix",
    "sort-package": "npx sort-package-json",
    "test": "pnpm build && vitest --environment jsdom run --coverage",
    "test:component": "vitest --environment jsdom run component/",
    "test:snapshot": "vitest --environment jsdom run snapshot/",
    "test:unit": "vitest --environment jsdom run unit/",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "supertokens-web-js": "0.5.0",
    "supertokens-website": "16.0.1",
    "@dzangolab/vue-config": "workspace:*",
    "@dzangolab/vue-i18n": "workspace:*",
    "@dzangolab/vue-layout": "workspace:*",
    "@dzangolab/vue-ui": "workspace:*",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@12deg/eslint-config": "0.1.11",
    "@12deg/tsconfig": "0.1.11",
    "@dzangolab/vue3-config": "0.10.10",
    "@dzangolab/vue3-form": "0.10.10",
    "@dzangolab/vue3-i18n": "0.10.10",
    "@dzangolab/vue3-layout": "0.10.10",
    "@dzangolab/vue3-tanstack-table": "0.10.10",
    "@dzangolab/vue3-ui": "0.10.10",
    "@iconify/vue": "4.3.0",
    "@types/node": "22.13.5",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "@vee-validate/i18n": "4.15.0",
    "@vee-validate/rules": "4.15.0",
    "@vee-validate/zod": "4.15.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vitest/coverage-istanbul": "3.0.6",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "12.7.0",
    "axios": "1.7.9",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-vue": "9.32.0",
    "mitt": "3.0.1",
    "prettier": "3.5.2",
    "typescript": "5.5.4",
    "vee-validate": "4.15.0",
    "vite": "6.1.1",
    "vitest": "3.0.6",
    "vue-tsc": "1.2.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@vitest/coverage-v8": "^0.34.5",
    "jsdom": "^22.1.0"
  },
  "peerDependencies": {
    "@dzangolab/vue3-config": "0.10.10",
    "@dzangolab/vue3-i18n": "0.10.10",
    "@dzangolab/vue3-layout": "0.10.10",
    "@dzangolab/vue3-ui": "0.10.10",
    "@vee-validate/i18n": ">=4.7.1",
    "@vee-validate/rules": ">=4.7.1",
    "@vueuse/core": ">=9.4.0",
    "axios": ">=0.26.1",
    "pinia": ">=2.0",
    "vee-validate": ">=4.7.1",
    "vue": ">=3.2",
    "vue-router": ">=4.0",
    "zod": ">=3.20.6"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}
